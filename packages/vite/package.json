        "zod": "^4.3.6"
    },
    "devDependencies": {
        "typescript": "^5.9.3",
        "vite": "^7.3.1",
        "vitest": "^4.0.18"
    },
    "peerDependencies": {
        "vite": ">=6.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "packageManager": "pnpm@10.28.1"
