{
    "name": "@pigeonhole/vite",
    "version": "0.0.0",
    "type": "module",
    "license": "MIT",
    "exports": {
        ".": {
            "development": "./src/index.ts",
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./virtual": {
            "types": "./virtual.d.ts"
        }
    },
    "files": ["dist", "src", "virtual.d.ts"],
    "scripts": {
        "build": "vite build",
        "clean": "rm -rf dist",
